<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.problemsolver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProblemSolver">

        <activity
            android:name=".FifteenSolver"
            android:exported="false"
        android:parentActivityName=".FifteenActivity" />


        <activity
            android:name=".FifteenActivity"
            android:exported="false"
        android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EightPuzzleSolverActivity"
            android:exported="false"
            android:parentActivityName=".EightPuzzleActivity" />
        <activity
            android:name=".FWGCSolverActivity"
            android:exported="false"
            android:parentActivityName=".FWGCActivity" />
        <activity
            android:name=".EightPuzzleActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FWGCActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>